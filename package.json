{
  "name": "training-notes-client",
  "version": "0.1.0",
  "description": "app to keep track of and take notes for your training sessions",
  "main": "./main.js",
  "scripts": {
    "clean": "rimraf ./public && mkdirp public/js",
    "dev:js": "NODE_ENV='development' rollup -c -w",
    "dev:server": "nodemon \"NODE_ENV='development' node ./main.js\"",
    "dev:resources": "NODE_ENV='development' node ./resources.js",
    "dev": "run-s clean && run-p dev:*",
    "prod:js": "NODE_ENV='production' rollup -c",
    "prod:resources": "NODE_ENV='production' node ./resources.js\"",
    "prod": "run-s clean && run-p prod:*",
    "server": "NODE_ENV='production' node ./main.js",
    "start": "npm run prod && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "training",
    "notes"
  ],
  "author": "kasperstorgaard",
  "license": "ISC",
  "dependencies": {
    "@polymer/lit-element": "^0.6.2",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "hasha": "^3.0.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "pug-code-gen": "^2.0.1",
    "pug-lexer": "^4.0.0",
    "pug-load": "^2.0.11",
    "pug-parser": "^5.0.0",
    "pug-runtime": "^2.0.4",
    "pwa-helpers": "^0.9.0",
    "spdy": "^4.0.0",
    "style-inject": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "chokidar": "^2.0.4",
    "glob": "^7.1.3",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-terser": "^3.0.0",
    "uglifycss": "0.0.29",
    "yargs": "^12.0.4"
  },
  "nodemonConfig": {
    "ext": "*.pug, *.js",
    "ignore": [
      "node_modules/**",
      "public/**",
      "@(app|components)/**.js"
    ]
  }
}
